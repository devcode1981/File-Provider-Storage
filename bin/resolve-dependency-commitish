#!/usr/bin/env bash

set -euo pipefail
IFS=$'\n\t'

version_file="${1}"
version="$(cat "${version_file}")"
version_size=${#version}

if [[ ${version_size} == 40 ]]; then
  echo "${version}"
  exit
fi

case "$version" in
  =*)
      echo "${version#"="}"
      ;;
  *)
      echo "v${version}"
      ;;
esac
